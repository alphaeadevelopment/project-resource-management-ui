{
  "name": "@alphaeadev/project-resource-management-ui",
  "version": "1.0.14",
  "description": "Project Resource Management",
  "scripts": {
    "test": "npm run test:ci -- --watch",
    "test:ci": "mocha-webpack --webpack-config config/webpack.mocha.js --require mocha.setup.js --glob \"*.spec.js*\" --reporter mochawesome --reporter-options reportDir=mochawesome-report-unit --recursive src",
    "build": "cross-env ENV=${ENV:-local} NODE_ENV=${NODE_ENV:-dev} webpack --config=config/webpack.dev.js",
    "start": "cross-env PORT=3003 PROXY_SERVER=${LOCAL_PROXY_SERVER:-http://localhost:3000} NODE_ENV=dev ENV=local nodemon -w src/server src/server/index.js",
    "serve": "cross-env ENV=${ENV:-local} NODE_ENV=${NODE_ENV:-dev} node src/server/index.js",
    "deploy:old": "npm version patch && git push && git push --tags && cross-env ENV=${ENV:-test} VERSION=`jq .version package.json` npm run deploy:inner",
    "deploy": "cross-env NODE_ENV=production PROXY_SERVER=${PROXY_SERVER_URI} npm run build && npm run stage && npm run deploy:inner",
    "stage": "$(mkdir staging || true) && cp -R dist/* staging",
    "deploy:inner": "aws --profile ${AWS_PROFILE:-default} s3 sync staging s3://${PRM_S3_BUCKET}",
    "deploy:cf": "aws --profile ${AWS_PROFILE:-default} cloudformation deploy --template-file cf/template.yaml --stack-name ${PRM_STACK_NAME} --capabilities CAPABILITY_IAM"
  },
  "keywords": [],
  "author": "Graham King <gking2224@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@alphaeadev/common-ui-components": "^1.0.3",
    "@alphaeadev/js-services": "^1.0.10",
    "body-parser": "^1.18.2",
