    "material-ui-icons": "^1.0.0-beta.17",
    "material-ui-schema-form": "^0.9.8",
    "node-rsa": "^0.4.2",
    "react": "^16.2.0",
    "react-datetime": "^2.14.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
