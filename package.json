{
  "name": "@alphaeadev/project-resource-management-ui",
  "version": "1.0.12",
  "description": "Online Project Resource Management Application",
  "scripts": {
    "test": "npm run test:ci -- --watch",
    "test:ci": "mocha-webpack --webpack-config config/webpack.mocha.js --require mocha.setup.js --glob \"*.spec.js*\" --reporter mochawesome --reporter-options reportDir=mochawesome-report-unit --recursive src",
    "build:prod": "export NODE_ENV=production && npm run use:stubs && npm run build",
    "build": "webpack --config=config/webpack.dev.js",
    "serve:dev": "nodemon -w src/server src/server/main.js",
    "serve": "node src/server/main.js",
    "deploy": "npm version patch && git push && git push --tags && cross-env ENV=${ENV:-test} VERSION=`jq .version package.json` npm run deploy:inner",
    "deploy:inner": "npm run deploy:bootstrap && npm run deploy:cf && npm run terminate:ec2",
    "deploy:bootstrap": "aws s3 cp config/cloudformation/ec2-bootstrap.sh s3://alphaea-uk-scripts/prr-ec2",
    "deploy:cf": "node-cf-deploy update-stack --name PRR-${ENV} -P Env=${ENV} -P NodeModule=project-resource-management-ui -P SubDomainName=prr-${ENV} -P EnvVars=\"NODE_ENV=production ENV=${ENV}\" -P BootstrapScriptName=node-bootstrap -P GitTag=v${VERSION} -f node_modules/\\@alphaeadev/cf-infra-code/cloudformation-templates/node-cf-template.json -S ",
    "terminate:ec2": "node-cf-deploy terminate-stack-instances -n PRR-${ENV}",
    "v": "echo $(cat package.json | jq .version | awk -F'\"' '{print $2}')",
    "use:stubs": "touch config/useStubs"
  },
  "keywords": [],
  "author": "Graham King <gking2224@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@alphaeadev/common-ui-components": "^1.0.2",
    "@alphaeadev/config-client": "^1.0.7",
    "@alphaeadev/js-services": "^1.0.5",
    "@alphaeadev/test-es6-npm-module": "^1.0.2",
    "@alphaeadev/test-npm-module": "^1.0.3",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "fetch-everywhere": "^1.0.5",
    "immutability-helper": "^2.6.4",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "node-rsa": "^0.4.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "sha.js": "^2.4.9",
    "uuid": "^3.2.1",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4"
  },
  "devDependencies": {
    "@alphaeadev/cf-infra-code": "^1.0.15",
    "@alphaeadev/node-cf-deploy": "^1.0.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.1",
    "mocha": "^4.1.0",
    "mocha-webpack": "^1.0.1",
    "mochawesome": "^3.0.1",
    "node-sass": "^4.7.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.6",
    "sinon-chai": "^2.14.0",
    "style-loader": "^0.19.1",
